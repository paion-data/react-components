{
  "ci": {
    "collect": {
      "startServerCommand": "yarn start",
      "startServerReadyPattern": "3000",
      "startServerReadyTimeout": 10000,

      "numberOfRuns": 1,

      "settings": {
        "skipAudits": ["robots-txt", "canonical", "tap-targets", "is-crawlable", "works-offline", "offline-start-url"]
      }
    }
  }
}
